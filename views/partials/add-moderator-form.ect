<form method="get" action="/kluby/hledat" class="form-inline search-by-content-form">
	<input type="hidden" name="_csrf" value="<%= @csrfToken %>">
	
	<div class="form-group">
		<label for="moderator-name" class="form-control-label">
			Jméno:
		</label>
		<input type="text" id="moderator-name" name="name" class="form-control form-control-sm" required />
	</div>

	<div class="form-group">
		<button type="submit" class="btn btn-secondary btn-sm">Přidat</button>
	</div>
</form>