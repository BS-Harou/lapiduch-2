<div class="clubs-table">
	<table class="table table-sm">
	<tbody>
	<% for club in @clubsList : %>
		<tr>
			<td>
				<a href="/kluby/<%= club.link %>"><%= club.name %></a>
				<span class="club-desc"><%= club.name %></span>
			</td>
			<td class="posts-amount">
				(0 příspěvků)
			</td>
		</tr>
	<% end %>
	</tbody>
	</table>
</div>

